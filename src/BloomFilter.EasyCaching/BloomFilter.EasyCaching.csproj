<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<AssemblyName>BloomFilter.EasyCaching</AssemblyName>
		<AssemblyTitle>BloomFilter.EasyCaching</AssemblyTitle>
		<PackageId>BloomFilter.EasyCaching.NetCore</PackageId>
		<TargetFrameworks>net462;netstandard2.0;net5.0;net6.0;net7.0</TargetFrameworks>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<Nullable>enable</Nullable>
		<NullableReferenceTypes>true</NullableReferenceTypes>
		<NoWarn>$(NoWarn);CS1591</NoWarn>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="EasyCaching.Core" Version="1.8.0" />
	</ItemGroup>


	<ItemGroup>
		<ProjectReference Include="..\BloomFilter\BloomFilter.csproj" />
	</ItemGroup>

	<PropertyGroup>
		<SignAssembly>False</SignAssembly>
	</PropertyGroup>

</Project>
